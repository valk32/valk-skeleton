{
	"name": "@brainandbones/skeleton",
	"version": "0.17.4",
	"description": "A SvelteKit component library.",
	"author": "endigo9740 <csimmons@brainandbonesllc.com>",
	"scripts": {
		"dev": "svelte-kit dev --port 3000",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"patch-package": "npm version patch && svelte-kit package",
		"publish": "npm publish ./package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"test": "vitest",
		"coverage": "vitest run --coverage"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^1.0.0-next.39",
		"@sveltejs/adapter-static": "^1.0.0-next.29",
		"@sveltejs/kit": "^1.0.0-next.322",
		"@tailwindcss/forms": "^0.5.0",
		"@testing-library/svelte": "^3.1.1",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"autoprefixer": "^10.4.5",
		"c8": "^7.11.2",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"highlight.js": "^11.5.1",
		"jsdom": "^19.0.0",
		"postcss": "^8.4.12",
		"postcss-load-config": "^3.1.4",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.47.0",
		"svelte-check": "^2.7.0",
		"svelte-preprocess": "^4.10.6",
		"svelte2tsx": "^0.5.9",
		"tailwindcss": "^3.0.24",
		"tslib": "^2.4.0",
		"typescript": "~4.5.4",
		"vitest": "^0.7.13",
		"vitest-svelte-kit": "0.0.6"
	},
	"type": "module"
}
